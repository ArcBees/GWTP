<assembly>
    <id>binary</id>
    <formats>
        <format>jar</format>
    </formats>
    <includeBaseDirectory>false</includeBaseDirectory>
    <dependencySets>
        <dependencySet>
            <outputFileNameMapping/>
            <unpack>true</unpack>
            <includes>
                <include>com.gwtplatform:*:*:</include>
            </includes>
            <excludes>
                <exclude>com.gwtplatform:*:*:sources</exclude>
            </excludes>
        </dependencySet>
    </dependencySets>
</assembly>