<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'
        xmlns:datepicker="urn:import:com.google.gwt.user.datepicker.client">

    <g:HTMLPanel>
        <datepicker:DatePicker ui:field="datePicker" debugId="extract-year-date-picker"/>
        <g:Button ui:field="extractYear" debugId="extract-year-button">Extract Year</g:Button>
        <p>
            Result:
            <g:InlineLabel ui:field="result" debugId="extract-year-result"/>
        </p>
    </g:HTMLPanel>
</ui:UiBinder>
