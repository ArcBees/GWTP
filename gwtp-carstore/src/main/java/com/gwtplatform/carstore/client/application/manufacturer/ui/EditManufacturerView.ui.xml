<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:PopupPanel glassEnabled="true" autoHideEnabled="true" animationEnabled="true">
        <g:HTMLPanel debugId="manufacturer-editor">
            <div>
                <g:InlineLabel>Name:</g:InlineLabel>
                <g:TextBox ui:field="name" debugId="manufacturer-input"/>
            </div>

            <div>
                <g:Button ui:field="save" debugId="manufacturer-save">Save</g:Button>
                <g:Button ui:field="close" debugId="manufacturer-close">Close</g:Button>
            </div>
        </g:HTMLPanel>
    </g:PopupPanel>
</ui:UiBinder>
