<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">

<module rename-to='gwtpsample'>
    <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Inherit the default GWT style sheet.  You can change       -->
    <!-- the theme of your GWT application by uncommenting          -->
    <!-- any one of the following lines.                            -->
    <inherits name='com.google.gwt.user.theme.standard.Standard'/>
    <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
    <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

    <!-- Specify the app entry point class.                         -->
    <entry-point class='com.gwtplatform.samples.basic.client.Gwtpsample'/>

    <inherits name='com.gwtplatform.dispatch.Dispatch'/>
    <inherits name='com.gwtplatform.mvp.Mvp'/>

    <!-- Specify the paths for translatable code                    -->
    <source path='client'/>
    <source path='shared'/>

    <generate-with class="com.gwtplatform.samples.basic.rebind.ApplicationControllerGenerator">
        <when-type-is class="com.gwtplatform.samples.basic.client.gin.ApplicationController"/>
    </generate-with>

    <define-configuration-property name="gin.ginjector" is-multi-valued="false"/>
    <set-configuration-property name="gin.ginjector" value="com.gwtplatform.samples.basic.client.gin.ClientGinjectorGenerated"/>

</module>
