<?xml version="1.0" encoding="UTF-8"?>
<module>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <generate-with class="com.philbeaudoin.gwtp.mvp.rebind.ProxyGenerator">
    <when-type-assignable class="com.philbeaudoin.gwtp.mvp.client.proxy.Proxy" />
  </generate-with>
  
  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
